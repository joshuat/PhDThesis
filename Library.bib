@article{turner_frequency_2002,
	title = {Frequency noise characterisation of narrow linewidth diode lasers},
	volume = {201},
	issn = {0030-4018},
	url = {http://www.sciencedirect.com/science/article/pii/S0030401801016893},
	doi = {10.1016/S0030-4018(01)01689-3},
	abstract = {We examine several approaches to laser frequency noise measurement in the frequency and time domains. Commonly employed methods such as optical frequency discrimination and the Allan variance are found to be complex, expensive, time-consuming, or incomplete. We describe a practical method of demodulating a laser beat note to measure a frequency noise spectrum, using a phase-locked loop frequency discriminator based on a single low-cost integrated circuit. This method measures the frequency noise spectrum of a laser directly and in detail and is insensitive to intensity fluctuations. The advantages of this scheme are demonstrated through measurement of the frequency noise spectrum for two external cavity diode lasers (ECDL), clearly distinguishing several common noise sources. These are isolated and removed, reducing the individual laser rms linewidth from 2 MHz to 450 kHz. The spectrum is used to calculate the Allan variance, which shows almost none of the important information.},
	number = {4–6},
	urldate = {2013-10-31},
	journal = {Optics Communications},
	author = {Turner, L.D and Weber, K.P and Hawthorn, C.J and Scholten, R.E},
	month = jan,
	year = {2002},
	keywords = {Diode lasers, Frequency discrimination, frequency noise},
	pages = {391--397},
	file = {ScienceDirect Snapshot:C\:\\Users\\Joshua Torrance\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\t3zcokfe.default\\zotero\\storage\\MS3MHUPH\\S0030401801016893.html:text/html;Turner et al. - 2002 - Frequency noise characterisation of narrow linewid.pdf:C\:\\Users\\Joshua Torrance\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\t3zcokfe.default\\zotero\\storage\\28VXMKJB\\Turner et al. - 2002 - Frequency noise characterisation of narrow linewid.pdf:application/pdf}
