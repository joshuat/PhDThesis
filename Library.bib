@article{kleine_buning_slow_2010,
	title = {A slow gravity compensated atom laser},
	volume = {100},
	issn = {0946-2171},
	url = {http://www.springerlink.com/content/fx1mk9034460k174/abstract/},
	doi = {10.1007/s00340-010-4078-7},
	abstract = {We report on a slow guided atom laser beam outcoupled from a Bose–Einstein condensate of 87 Rb atoms in a hybrid trap. The acceleration of the atom laser beam can be controlled by compensating the gravitational acceleration and we reach residual accelerations as low as 0.0027 g. The outcoupling mechanism allows for the production of a constant flux of 4.5×10 6 atoms per second and due to transverse guiding we obtain an upper limit for the mean beam width of 4.6 {\textbackslash}mu m. The transverse velocity spread is only 0.2 mm/s and thus an upper limit for the beam quality parameter is M 2 =2.5. We demonstrate the potential of the long interrogation times available with this atom laser beam by measuring the trap frequency in a single measurement. The small beam width together with the long evolution and interrogation time makes this atom laser beam a promising tool for continuous interferometric measurements.},
	number = {1},
	urldate = {2012-05-08},
	journal = {Applied Physics B: Lasers and Optics},
	author = {Kleine Büning, G. and Will, J. and Ertmer, W. and Klempt, C. and Arlt, J.},
