	journal = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
	author = {Dwyer, J. R. and Hebeisen, C. T. and Ernstorfer, R. and Harb, M. and Deyirmenjian, V. B. and Jordan, R. E. and Dwayne Miller, R. J.},
	month = mar,
