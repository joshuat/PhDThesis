@article{ayyer_macromolecular_2016,
	title = {Macromolecular diffractive imaging using imperfect crystals},
	volume = {530},
	copyright = {© 2016 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {0028-0836},
	url = {http://www.nature.com.ezp.lib.unimelb.edu.au/nature/journal/v530/n7589/full/nature16949.html?foxtrotcallback=true},
	doi = {10.1038/nature16949},
	abstract = {The three-dimensional structures of macromolecules and their complexes are mainly elucidated by X-ray protein crystallography. A major limitation of this method is access to high-quality crystals, which is necessary to ensure X-ray diffraction extends to sufficiently large scattering angles and hence yields information of sufficiently high resolution with which to solve the crystal structure. The observation that crystals with reduced unit-cell volumes and tighter macromolecular packing often produce higher-resolution Bragg peaks suggests that crystallographic resolution for some macromolecules may be limited not by their heterogeneity, but by a deviation of strict positional ordering of the crystalline lattice. Such displacements of molecules from the ideal lattice give rise to a continuous diffraction pattern that is equal to the incoherent sum of diffraction from rigid individual molecular complexes aligned along several discrete crystallographic orientations and that, consequently, contains more information than Bragg peaks alone. Although such continuous diffraction patterns have long been observed—and are of interest as a source of information about the dynamics of proteins—they have not been used for structure determination. Here we show for crystals of the integral membrane protein complex photosystem II that lattice disorder increases the information content and the resolution of the diffraction pattern well beyond the 4.5-ångström limit of measurable Bragg peaks, which allows us to phase the pattern directly. Using the molecular envelope conventionally determined at 4.5 ångströms as a constraint, we obtain a static image of the photosystem II dimer at a resolution of 3.5 ångströms. This result shows that continuous diffraction can be used to overcome what have long been supposed to be the resolution limits of macromolecular crystallography, using a method that exploits commonly encountered imperfect crystals and enables model-free phasing.},
	language = {en},
	number = {7589},
	urldate = {2017-09-08},
	journal = {Nature},
	author = {Ayyer, Kartik and Yefanov, Oleksandr M. and Oberthür, Dominik and Roy-Chowdhury, Shatabdi and Galli, Lorenzo and Mariani, Valerio and Basu, Shibom and Coe, Jesse and Conrad, Chelsie E. and Fromme, Raimund and Schaffer, Alexander and Dörner, Katerina and James, Daniel and Kupitz, Christopher and Metz, Markus and Nelson, Garrett and Xavier, Paulraj Lourdu and Beyerlein, Kenneth R. and Schmidt, Marius and Sarrou, Iosifina and Spence, John C. H. and Weierstall, Uwe and White, Thomas A. and Yang, Jay-How and Zhao, Yun and Liang, Mengning and Aquila, Andrew and Hunter, Mark S. and Robinson, Joseph S. and Koglin, Jason E. and Boutet, Sébastien and Fromme, Petra and Barty, Anton and Chapman, Henry N.},
	month = feb,
	year = {2016},
	keywords = {Biological physics, Nanocrystallography, Optics and Photonics, X-ray crystallography, X-rays},
	pages = {202--206},
	file = {Full Text PDF:C\:\\Users\\Joshua Torrance\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\t3zcokfe.default\\zotero\\storage\\59QHC7F6\\Ayyer et al. - 2016 - Macromolecular diffractive imaging using imperfect.pdf:application/pdf;Snapshot:C\:\\Users\\Joshua Torrance\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\t3zcokfe.default\\zotero\\storage\\HRCZAZ35\\nature16949.html:text/html}
