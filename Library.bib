@article{hughes_polarization_2009,
	title = {Polarization {Spectroscopy} of {Alkali}-{Metal} {Atoms}},
	volume = {263},
	abstract = {A study of polarization spectroscopy of alkali-metal atoms (Rb, Cs) on the D2 line is presented. In polarization spectroscopy a weak probe beam analyses the birefringence induced in a sample by a strong counter progagating cirularly polarized pump beam. The signal is in the form of a dispersion curve. We outline a numerical treatment of optical pumping and use it to make predictions about the general form of polarization spectra in alkali-mela atoms. Theoretical spectra for the D2 transtions in Rb and Cs are generated. Comparison of experiments conducted with diode lasers in room temperature vapours and theory demonstrates that the model accurately reproduces spectra of tansitions from the upper hyperfine level of the ground state. Among these, the close transition F -{\textgreater} F' = F+1 dominates. The polarization spectrum provides an ideal reference, or "error" signal for frequency stabilization of a laser.},
	journal = {New Trends in Quantum Coherence and Nonlinear Optics},
	author = {Hughes, Ifan G.},
	year = {2009},
	pages = {149--169}
}

