	pages = {48--52},
	file = {Full Text PDF:C\:\\Users\\Joshua Torrance\\Zotero\\storage\\KNBZ9RL6\\Schioppo et al. - 2017 - Ultrastable optical clock with two cold-atom ensem.pdf:application/pdf;Snapshot:C\:\\Users\\Joshua Torrance\\Zotero\\storage\\QGVYZGBX\\nphoton.2016.html:text/html}
