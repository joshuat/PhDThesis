	keywords = {atomic level view of dynamics, electron-electron correlations, femtosecond electron diffraction, laser ponderomotive effects, Space charge effects, transition states},
	pages = {741--778},
	file = {Snapshot:C\:\\Users\\Joshua Torrance\\Zotero\\storage\\UBQBIPFT\\741.html:text/html}
