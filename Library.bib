@article{van_der_geer_ultracold_2009,
	title = {Ultracold {Electron} {Source} for {Single}-{Shot}, {Ultrafast} {Electron} {Diffraction}},
	volume = {15},
	issn = {1435-8115, 1431-9276},
	url = {https://www-cambridge-org.ezp.lib.unimelb.edu.au/core/journals/microscopy-and-microanalysis/article/ultracold-electron-source-for-singleshot-ultrafast-electron-diffraction/EEFCB6DD649AC717BAE6FB2A9D95A722},
	doi = {10.1017/S143192760909076X},
	abstract = {AbstractUltrafast electron diffraction (UED) enables studies of structural dynamics at atomic length and timescales, i.e., 0.1 nm and 0.1 ps, in single-shot mode. At present UED experiments are based on femtosecond laser photoemission from solid state cathodes. These photoemission sources perform excellently, but are not sufficiently bright for single-shot studies of, for example, biomolecular samples. We propose a new type of electron source, based on near-threshold photoionization of a laser-cooled and trapped atomic gas. The electron temperature of these sources can be as low as 10 K, implying an increase in brightness by orders of magnitude. We investigate a setup consisting of an ultracold electron source and standard radio-frequency acceleration techniques by GPT tracking simulations. The simulations use realistic fields and include all pairwise Coulomb interactions. We show that in this setup 120 keV, 0.1 pC electron bunches can be produced with a longitudinal emittance sufficiently small for enabling sub-100 fs bunch lengths at 1\% relative energy spread. A transverse root-mean-square normalized emittance of εx = 10 nm is obtained, significantly better than from photoemission sources. Correlations in transverse phase-space indicate that the transverse emittance can be improved even further, enabling single-shot studies of biomolecular samples.},
	number = {4},
	urldate = {2017-09-18},
	journal = {Microscopy and Microanalysis},
	author = {van der Geer, S. B. and Loos, M. J. de and Vredenbregt, E. J. D. and Luiten, O. J.},
	month = aug,
	year = {2009},
	pages = {282--289},
	file = {Full Text PDF:C\:\\Users\\Joshua Torrance\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\t3zcokfe.default\\zotero\\storage\\4R3KS6SP\\Geer et al. - 2009 - Ultracold Electron Source for Single-Shot, Ultrafa.pdf:application/pdf;Snapshot:C\:\\Users\\Joshua Torrance\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\t3zcokfe.default\\zotero\\storage\\4QV56263\\EEFCB6DD649AC717BAE6FB2A9D95A722.html:text/html}
}

