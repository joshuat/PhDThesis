@article{longchamp_how_2015,
	title = {How to image a single protein},
	url = {http://arxiv.org/abs/1512.08958},
	abstract = {Imaging a single protein has been a long-standing dream for advancing structural biology and with this various fields in natural science. In particular, revealing the distinct conformations of an individual protein is of outermost importance. To do so, one needs to master and combine three requirements. At first, a method for isolating individual proteins for further inspection has to be at hand; quite the opposite to the current challenge of assembling proteins into a crystal for X-ray analysis. Furthermore, technologies are required for keeping a single protein fixed in space long enough to accumulate sufficient structural information from a scattering experiment. Last but not least, gentle radiation with a wavelength small enough to uncover structural details while ensuring that radiation damage does not decompose the protein during observation as it is available by low-energy electron holography is vital for imaging. Here we show that soft-landing electrospray beam deposition allows for specific selection and sound deposition of individual proteins and protein complexes onto ultraclean freestanding graphene in an ultra-high vacuum environment. Due to the fact that graphene is transparent for low-energy electrons and since the latter do not damage biological molecules, we were able to acquire high signal-to-noise ratio electron holograms of individual proteins (Cytochrome C and BSA) as well as of protein complexes (haemoglobin). The numerical hologram reconstructions reveal the overall shape of single proteins. With this, images of individual folded proteins and protein complexes, not being the result of an averaging process, have been obtained for the first time.},
	urldate = {2016-01-18},
	journal = {arXiv:1512.08958 [cond-mat, physics:physics, q-bio]},
	author = {Longchamp, Jean-Nicolas and Rauschenbach, Stephan and Abb, Sabine and Escher, Conrad and Latychevskaia, Tatiana and Kern, Klaus and Fink, Hans-Werner},
	month = dec,
	year = {2015},
	note = {arXiv: 1512.08958},
	keywords = {Condensed Matter - Soft Condensed Matter, Physics - Biological Physics, Physics - Instrumentation and Detectors, Quantitative Biology - Quantitative Methods},
	file = {arXiv\:1512.08958 PDF:C\:\\Users\\Joshua Torrance\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\t3zcokfe.default\\zotero\\storage\\W4S4QM2N\\Longchamp et al. - 2015 - How to image a single protein.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\Joshua Torrance\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\t3zcokfe.default\\zotero\\storage\\H2KUUE3A\\Longchamp et al. - 2015 - How to image a single protein.html:text/html}
}

