	year = {2016},
	keywords = {atomic, Laser stabilization, Spectroscopy},
	pages = {11396--11406},
	file = {Full Text PDF:C\:\\Users\\Joshua Torrance\\Zotero\\storage\\3X7UWNUD\\Torrance et al. - 2016 - Sub-kilohertz laser linewidth narrowing using pola.pdf:application/pdf;Snapshot:C\:\\Users\\Joshua Torrance\\Zotero\\storage\\4K9R79PQ\\abstract.html:text/html}
